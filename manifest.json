{
	"manifest_version": 3,
	"name": "New Tab Extension",
	"description": "Set the new tab url",
	"version": "1.0.0",
	"author": "Spectra",
	"chrome_url_overrides": {
		"newtab": "src/new_tab/new_tab.html"
	},
	"options_page": "src/options/options.html",
	"icons": {
		"16": "icons/icon_16.png",
		"128": "icons/icon_16.png"
	},
	"permissions": [
		"favicon",
		"storage"
	]
}